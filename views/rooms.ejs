

<div id="roomsContainer" class="container">
	<div id="roomsCount"><span id="numberRooms">0</span> ROOMS <i class="fa fa-bar-chart"></i></div>
    <div class="btn-group btn-toggle" id="switch">
        <button class="btn btn-sm btn-primary active" id="list" data-toggle="tooltip" data-placement="top" title="Show streams in list"><span class="glyphicon glyphicon-menu-hamburger"></span> </button>
        <button class="btn btn-sm btn-default" id="grid" data-toggle="tooltip" data-placement="top" title="Show streams in grid"><span class="glyphicon glyphicon-th-large"></span></button>
        <a id="historical" href="history"><button class="btn btn-sm btn-default" id="grid" data-toggle="tooltip" data-placement="top" title="Historical data panel"><span class="glyphicon glyphicon-time"></span></button></a>
    </div>

	<div class="row" id="search">
       <div id="custom-search-input">
            <div class="input-group col-md-12">
                <input id="searchBar" type="text" class="search-query form-control" placeholder="Search" />
                <span class="input-group-btn">
                    <button class="btn btn-danger" type="button">
                        <span class=" glyphicon glyphicon-search"></span>
                    </button>
                </span>
            </div>
        </div>
	</div>
	<div id="rooms" class="row row-centered">

	</div>

</div>


<script>

  var rooms = <%- JSON.stringify(rooms) %>;

  paintRoomsList();

</script>
